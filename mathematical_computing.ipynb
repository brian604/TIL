    "from sympy import *\n",
    "x = symbols('x')\n",
    "init_printing(use_unicode = True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "a = expand(x**8 + (1-x)**2)\n",
    "b = expand(x**9 + (1-x))\n",
    "polyn = 45*a+10*b+1*(x**10)\n",
    "win = 10*polyn\n",
    "lose = 8*(1-polyn)"
   ]
  },
